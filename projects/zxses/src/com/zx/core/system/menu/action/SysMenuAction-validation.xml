<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="sysMenu.code">
		<field-validator type="requiredstring">
			<message>【菜单编号】不能为空！</message>
		</field-validator>
	</field>
	<field name="sysMenu.name">
		<field-validator type="requiredstring">
			<message>【菜单名称】不能为空！</message>
		</field-validator>
	</field>
	<field name="sysMenu.url">
		<field-validator type="requiredstring">
			<message>【菜单URL】不能为空！</message>
		</field-validator>
	</field>
	<field name="sysMenu.recordOrderStr">
		<field-validator type="requiredstring">
			<message>【排 序】不能为空！</message>
		</field-validator>
	</field>
</validators>